{
    "scopeName": "source.bel-injection-json",
    "injectionSelector": "L:source.json",
    "patterns": [
        {
            "include": "#citation"
        },
        {
            "include": "#assertions"
        },
        {
            "include": "#annotations"
        },
        {
            "include": "#nanopub"
        },
        {
            "include": "#metadata"
        },
        {
            "include": "#evidence"
        }
    ],
    "repository": {
        "citation": {
            "match": "\"citation\"",
            "name":"entity.name.function.bel.citation"
        },
        "assertions": {
            "match": "\"assertions\"",
            "name":"entity.name.function.bel.assertions"
        },
        "annotations": {
            "match": "\"annotations\"",
            "name":"entity.name.function.bel.annotations"
        },
        "evidence": {
            "match": "\"evidence\"",
            "name":"entity.name.function.bel.evidence"
        },
        "nanopub": {
            "match": "\"nanopub\"",
            "name":"entity.name.class.bel.nanopub"
        },
        "metadata": {
            "match": "\"metadata\"",
            "name":"entity.name.class.bel.metadata"
        }
    }
}